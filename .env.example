# Environment Variables

# Database
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# JWT Secret
JWT_SECRET=your-secret-key-change-this-in-production

# Cloudflare R2
# Format: https://pub-<account-hash>.r2.dev untuk public domain
# atau https://<custom-domain> jika punya custom domain
R2_DOMAIN=https://pub-YOUR_ACCOUNT_HASH.r2.dev
R2_BUCKET_NAME=your-bucket-name
